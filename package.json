{
  "name": "machine",
  "version": "15.0.0-0",
  "description": "Build your project with machines: a standard format for JavaScript logic.",
  "main": "index.js",
  "scripts": {
    "test": "nodever=`node -e \"console.log('\\`node -v\\`'[1]);\"` && if [ $nodever != \"0\" ]; then npm run lint; fi && node ./node_modules/mocha/bin/mocha -R dot test/*.test.js",
    "lint": "node ./node_modules/eslint/bin/eslint . --max-warnings=0 --ignore-pattern 'test/'",
    "bench": "node ./node_modules/mocha/bin/mocha -R dot --recursive -b test/benchmarks/"
  },
  "docs": {
    "url": "http://node-machine.org/spec"
  },
  "author": "The Sails Company",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/node-machine/machine.git"
  },
  "bugs": {
    "url": "http://sailsjs.com/bugs"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 2.0.0"
  },
  "dependencies": {
    "@sailshq/lodash": "^3.10.2",
    "flaverr": "^1.1.1",
    "parley": "^2.3.1",
    "rttc": "^9.8.1"
  },
  "devDependencies": {
    "async": "2.0.1",
    "benchmark": "2.1.0",
    "eslint": "3.5.0",
    "mocha": "3.0.2",
    "test-machinepack-mocha": "^2.1.3"
  }
}
